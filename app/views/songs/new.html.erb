<%= form_with url: "/artists/#{@artist_id}/songs", method: :post, local: true do |f| %>
  <%= f.label :title, 'Title' %>
  <%= f.text_field :title %>

  <%= f.label :length, 'Length' %>
  <%= f.text_field :length %>

  <%= f.label :play_count, 'Play Count' %>
  <%= f.text_field :play_count %>

  <%= f.submit 'Create Song' %>
<% end %>
